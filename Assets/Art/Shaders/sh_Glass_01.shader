		uniform float2 _Tiling;
		uniform samplerCUBE _TextureSample1;
		uniform float _Float0;
		uniform float4 _Color0;
		uniform float _FresnelScale;
		uniform float _FresnelPower;

		inline half4 LightingUnlit( SurfaceOutput s, half3 lightDir, half atten )
		{
			return half4 ( 0, 0, 0, s.Alpha );
		}

		void surf( Input i , inout SurfaceOutput o )
		{
			float2 panner13 = ( 1.0 * _Time.y * float2( 0.1,0.1 ) + float2( 0,0 ));
			float2 uv_TexCoord14 = i.uv_texcoord * _Tiling + panner13;
			float3 ase_worldReflection = normalize( i.worldRefl );
			o.Emission = ( ( tex2D( _TextureSample0, uv_TexCoord14 ) + texCUBE( _TextureSample1, ase_worldReflection ) ) * _Float0 * _Color0 ).rgb;
