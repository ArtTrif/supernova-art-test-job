		_TextureSample1("Texture Sample 1", CUBE) = "white" {}
		_Float0("Float 0", Float) = 1
		_Color0("Color 0", Color) = (1,1,1,0)
		_Tiling("Tiling", Vector) = (1,1,0,0)
		_FresnelScale("FresnelScale", Float) = 1
		_FresnelPower("FresnelPower", Float) = 3
